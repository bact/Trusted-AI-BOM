  "credentialSubject": {
    "type": "rule",
    "rule": [
      "% Retrieve all poisoninging attestations for an AI system",
      "ai_system_has_data_poisoning(AISystemId, Attestations) :-",
      "    findall(",
      "        (component(ComponentHash, DataVcId), PoisoningingDetails),",
      "        (data_in_inferencing_system(DataVcIds, AISystemId),",
      "         member(DataVcId, DataVcIds),",
      "         poisoning_attestation(_, attestation(PoisoningingDetails, \"poisoning\"), component(ComponentHash, DataVcId))),",
      "        Attestations",
      "    )."
    ]
  }
}
